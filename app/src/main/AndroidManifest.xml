<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fungwahtoolsdemo">

    <application
        android:name=".application.FungWahApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".main.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".animdemo.activity.TweenActivity" />
        <activity android:name=".animdemo.activity.ValueAnimActivity" />
        <activity android:name=".animdemo.activity.ObjectAnimatorActivity"/>
        <activity android:name=".animdemo.activity.LollipopAnimationActivity"/>

        <activity android:name=".animdemo.activity.court1.ExplodeActivity"/>
        <activity android:name=".animdemo.activity.court1.SlideActivity"/>
        <activity android:name=".animdemo.activity.court1.FadeActivity"/>
        <activity android:name=".animdemo.activity.court1.ShareActivity"/>

        <activity android:name=".animdemo.activity.court2.TweenTestAnimActivity"/>
        <activity android:name=".animdemo.activity.court2.VisibilityTestActivity"/>

    </application>

</manifest>